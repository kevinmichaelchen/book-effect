{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": ["eslint.config.js"],
      "project": ["*.{js,ts}"]
    },
    "packages/core": {
      "entry": ["src/index.ts"],
      "project": ["src/**/*.ts"]
    },
    "packages/google-books": {
      "entry": ["src/index.ts", "src/**/*.test.ts"],
      "project": ["src/**/*.ts"]
    },
    "packages/hardcover": {
      "entry": ["src/index.ts", "src/**/*.test.ts"],
      "project": ["src/**/*.ts"]
    },
    "packages/open-library": {
      "entry": ["src/index.ts", "src/**/*.test.ts"],
      "project": ["src/**/*.ts"]
    }
  },
  "ignore": ["**/dist/**"],
  "ignoreDependencies": ["@swc/helpers"],
  "ignoreBinaries": ["build"]
}
